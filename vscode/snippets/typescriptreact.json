{
	// Place your snippets for typescriptreact here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Print to console with DEBUG flag": {
		"prefix": "ld",
		"body": [
			"console.log(\"[DEBUG][$TM_FILENAME_BASE] $1: \", $1);$0"
		],
		"description": "Log output with DEBUG flag to console"
	},
	"Print to console a string with DEBUG flag": {
		"prefix": "ldb",
		"body": [
			"console.log(\"[DEBUG][$TM_FILENAME_BASE] $1\");$0"
			],
			"description": "Log string with DEBUG flag to console"
		},
	"Print to console a JSON object with DEBUG flag": {
		"prefix": "ldj",
		"body": [
			"console.log(\"[DEBUG][$TM_FILENAME_BASE](JSON) $1: \", JSON.stringify($1, null, 2));$0"
		],
		"description": "Log output with DEBUG flag to console"
	},
	"Print to console with DEBUG flag followed by OR condition": {
		"prefix": "ldo",
		"body": [
			"console.log(\"[DEBUG][$TM_FILENAME_BASE] $1: \", $1) || $0"
		],
		"description": "Log output with DEBUG flag of an object to console with OR ending"
	},
}
